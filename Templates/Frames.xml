<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Frame name = "SS_ClearFrame_Template" virtual="true">
    <Layers>
      <Layer>
        <FontString parentKey = "title" font="Fonts\FRIZQT__.TTF">
          <Color r="1" g="1" b="1" a="1"/>
          <FontHeight val="12"/>
          <Anchors>
            <Anchor point="TOP" x="0" y="-20" />
          </Anchors>
          <Shadow>
            <Offset x="-1" y="-1" />
            <Color r="0" g="0" b="0"/>
          </Shadow>
        </FontString>
      </Layer>
    </Layers>
  </Frame>
  <Frame name = "SS_FrameWithoutBG_Template" virtual="true">
    <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
      <BackgroundInsets>
        <AbsInset left="8" right="8" top="8" bottom="8"/>
      </BackgroundInsets>
      <TileSize>
        <AbsValue val="32"/>
      </TileSize>
      <EdgeSize>
        <AbsValue val="32"/>
      </EdgeSize>
    </Backdrop>
    <Layers>
      <Layer>
        <FontString parentKey = "title" font="Fonts\FRIZQT__.TTF">
          <Color r="1" g="1" b="1" a="1"/>
          <FontHeight val="12"/>
          <Anchors>
            <Anchor point="TOP" x="0" y="-20" />
          </Anchors>
          <Shadow>
            <Offset x="-1" y="-1" />
            <Color r="0" g="0" b="0"/>
          </Shadow>
        </FontString>
      </Layer>
    </Layers>
  </Frame>
  <Frame name = "SS_CloseableFrameWithoutBG_Template" virtual="true">
    <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
      <BackgroundInsets>
        <AbsInset left="8" right="8" top="8" bottom="8"/>
      </BackgroundInsets>
      <TileSize>
        <AbsValue val="32"/>
      </TileSize>
      <EdgeSize>
        <AbsValue val="32"/>
      </EdgeSize>
    </Backdrop>
    <Layers>
      <Layer>
        <FontString parentKey = "title" font="Fonts\FRIZQT__.TTF">
          <Color r="1" g="1" b="1" a="1"/>
          <FontHeight val="12"/>
          <Anchors>
            <Anchor point="TOP" x="0" y="-20" />
          </Anchors>
          <Shadow>
            <Offset x="-1" y="-1" />
            <Color r="0" g="0" b="0"/>
          </Shadow>
        </FontString>
      </Layer>
    </Layers>
    <Frames>
      <Button name="$parent_Close_Button" inherits="OptionsButtonTemplate" text="x">
        <Size x = "20" y = "20" />
        <Anchors>
          <Anchor point="TOPRIGHT">
            <Offset x="-20" y="-20"/>
          </Anchor> 
        </Anchors>
        <Scripts>
          <OnClick>
            self:GetParent():Hide()
          </OnClick>
        </Scripts>
      </Button>
    </Frames>
  </Frame>
  <Frame name = "SS_Stats_ButtonControlls_Template" virtual="true">
    <Size x = "80" y = "32" />
    <Frames>
      <Button inherits="SS_AddButton_Template" parentKey = "addBtn" />
      <Button inherits="SS_ReduceButton_Template" parentKey = "reduceBtn" />
      <Button inherits="SS_RemoveButton_Template" parentKey = "removeBtn" />
      <Button inherits="SS_InfoButton_Template" parentKey = "infoBtn" />
    </Frames>
  </Frame>
  <Frame name = "SS_Armor_ArmorBlock_Template" virtual="true">
    <Size><AbsDimension x="180" y="160"/></Size>
    <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
      <BackgroundInsets>
        <AbsInset left="8" right="8" top="8" bottom="8"/>
      </BackgroundInsets>
      <TileSize>
        <AbsValue val="32"/>
      </TileSize>
      <EdgeSize>
        <AbsValue val="32"/>
      </EdgeSize>
    </Backdrop>
    <Layers>
      <Layer>
        <FontString parentKey = "name" font="Fonts\FRIZQT__.TTF">
          <Color r="1" g="1" b="1" a="1"/>
          <FontHeight val="12"/>
          <Anchors>
            <Anchor point="TOPLEFT" x="44" y="-15" />
          </Anchors>
          <Shadow>
            <Offset x="-1" y="-1" />
            <Color r="0" g="0" b="0"/>
          </Shadow>
        </FontString>
      </Layer>
    </Layers>
    <Frames>
      <CheckButton parentKey = "check" inherits="OptionsCheckButtonTemplate" checked="false">
        <Anchors>
          <Anchor point="TOPLEFT">
            <Offset>
              <AbsDimension x="14" y="-10"></AbsDimension>
            </Offset>
          </Anchor>
        </Anchors>
      </CheckButton>
    </Frames>
  </Frame>
</Ui>